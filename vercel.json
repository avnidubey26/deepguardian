{
  "builds": [
    {
      "src": "api/scan.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/scan",
      "dest": "api/scan.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}